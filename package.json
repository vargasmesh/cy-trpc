{
  "name": "cy-trpc",
  "version": "0.0.1",
  "description": "A dynamic Cypress Interceptor for tRPC routers.",
  "author": "Lucas Vargas Noronha <lvargasnoronha@gmail.com>",
  "license": "MIT",
  "keywords": [
    "cypress",
    "trpc",
    "testing",
    "stub"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && npm publish"
  },
  "peerDependencies": {
    "@trpc/server": "^10.30.0",
    "cypress": "^10.6.0"
  },
  "dependencies": {
    "@trpc/client": "^10.30.0",
    "@trpc/react-query": "^10.30.0",
    "@trpc/server": "^10.30.0",
    "cypress": "^10.6.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.5",
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@cypress/webpack-preprocessor": "^5.17.1",
    "@types/node": "^20.3.1",
    "babel-loader": "^9.1.2",
    "bumpp": "^9.1.1",
    "eslint": "^8.42.0",
    "esno": "^0.16.3",
    "lint-staged": "^13.2.2",
    "ts-loader": "^9.4.3",
    "typescript": "^5.1.3",
    "unbuild": "^1.2.1",
    "vitest": "^0.32.0",
    "webpack": "^5.87.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
